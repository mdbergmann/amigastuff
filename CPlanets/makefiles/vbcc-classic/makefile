# makefile for GNU make (automatically generated by makemake)
# NOTE: indent with TAB in GNU makefiles!

# paths are relative to the project directory (current directory during make)

EXE    = bin/vbcc-classic/CPlanets
OBJDIR = o/vbcc-classic/

# compiler and linker

CC = vbcc:bin/vc
LD = vbcc:bin/vc

# target 'all' (default target)

all : $(EXE)

$(OBJDIR)main.o : main.c
	$(CC) $(shell devkits:c/vbccprefs) -c -o $(OBJDIR)main.o main.c

OBJS = $(OBJDIR)main.o

$(EXE) : $(OBJS)
	$(LD) $(shell devkits:c/vbccprefs) -o $(EXE) $(OBJS)

# target 'clean'

clean:
	-delete $(EXE)
	-delete $(OBJDIR)\*.o
